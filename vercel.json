{
    "version": 2,
    "builds": [
      {
        "src": "website/__init__.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "website/__init__.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production"
    }
  }
  